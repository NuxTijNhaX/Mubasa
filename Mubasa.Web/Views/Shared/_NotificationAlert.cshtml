 
<script src="~/lib/notiflix/dist/notiflix-notify-aio-3.2.6.min.js"></script>

@if(TempData["success"] != null) 
{
    <script>
        Notiflix.Notify.success('@TempData["success"]');
    </script>
}

@if(TempData["failure"] != null) 
{
    <script>
        Notiflix.Notify.failure('@TempData["failure"]');
    </script>
}

@if (TempData["info"] != null)
{
    <script>
        Notiflix.Notify.info('@TempData["info"]');
    </script>
}